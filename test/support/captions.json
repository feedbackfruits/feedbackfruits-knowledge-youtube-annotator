    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P238.91S",
    "duration": "P3.59S",
    "text": "3 times negative 7.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI0Mi41Uy1QNC4yOVM=",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P242.5S",
    "duration": "P4.29S",
    "text": "And our denominator is going to be 2 times 10.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI0Ni43OVMtUDEuMVM=",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P246.79S",
    "duration": "P1.1S",
    "text": "2 times 10.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI0Ny44OVMtUDQuNjZT",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P247.89S",
    "duration": "P4.66S",
    "text": "",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI1Mi41NVMtUDEuNlM=",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P252.55S",
    "duration": "P1.6S",
    "text": "So this is going to be the numerator.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI1NC4xNVMtUDQuMzNT",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P254.15S",
    "duration": "P4.33S",
