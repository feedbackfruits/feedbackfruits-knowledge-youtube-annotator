  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDIyMC4xUy1QMi4zNVM=",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P220.1S",
    "duration": "P2.35S",
    "text": "would work with improper fractions.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDIyMi40NVMtUDYuODJT",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P222.45S",
    "duration": "P6.82S",
