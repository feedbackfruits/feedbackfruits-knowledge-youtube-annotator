  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI2NS41NVMtUDEuNTdT",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P265.55S",
    "duration": "P1.57S",
    "text": "Well, that is just 20.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI2Ny4xMlMtUDIuNTlT",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P267.12S",
    "duration": "P2.59S",
    "text": "So this is negative 21/20.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI2OS43MVMtUDMuNFM=",
