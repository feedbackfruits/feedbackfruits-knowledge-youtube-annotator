  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI2Mi4wMVMtUDEuNjVT",
    "@type": [ "https://knowledge.express/VideoCaption" ],
    "startsAfter": "P262.01S",
    "duration": "P1.65S",
    "text": "Negative 21.",
    "language": "en"
  },
  {
    "@id": "https://video.google.com/timedtext?v=pi3WWQ0q6Lc&lang=en#UDI2My42NlMtUDEuODlT",
