{
  "name": "feedbackfruits-knowledge-youtube-annotator",
  "version": "0.2.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prestart": "npm run build",
    "start": "node lib",
    "pretest": "npm run clean && tsc --inlineSourceMap --outDir lib",
    "test": "nyc ava --verbose",
    "posttest": "npm run clean && npm run build",
    "watch-test": "nodemon -w test -x 'npm run test'",
    "coverage": "codecov",
    "watch": "nodemon lib"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Joost Verdoorn",
  "license": "MIT",
  "dependencies": {
    "@joegesualdo/get-youtube-subtitles-node": "0.0.13",
    "@types/node": "^9.4.0",
    "@types/node-uuid": "^0.0.28",
    "@types/xml2json": "^0.10.0",
    "cayley": "^0.2.1",
    "dotenv": "^4.0.0",
    "feedbackfruits-knowledge-engine": "feedbackfruits/feedbackfruits-knowledge-engine#feature/ontology",
    "googleapis": "^25.0.0",
    "memux": "github:knowledge-express/memux#~0.4.1",
    "node-fetch": "^1.6.3",
    "node-uuid": "^1.4.8",
    "p-queue": "^0.2.0",
    "pm2": "^2.1.5",
    "rxjs": "^5.0.0-rc.4",
    "striptags": "^3.0.1",
    "xml2json": "^0.11.0",
    "ytdl-core": "^0.20.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "blue-tape": "^0.2.0",
    "codecov": "^1.0.1",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0",
    "tap": "^5.7.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
